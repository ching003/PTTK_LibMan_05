    </servlet>
    <servlet>
        <servlet-name>BorrowDocumentServlet</servlet-name>
        <servlet-class>servlet.BorrowDocumentServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DocumentServlet</servlet-name>
        <servlet-class>servlet.DocumentServlet</servlet-class>
    </servlet>
